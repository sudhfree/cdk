CREATE TABLE IF NOT EXISTS dw_royalties.subscriber_snapshot(
    accountnumber INT,
    profile_id BIGINT,
    action VARCHAR(50),
    source VARCHAR(50),
    subscription_tier VARCHAR(20),
    expiration_date DATE,
    date_start VARCHAR(30),
    date_end VARCHAR(30),
    previous_subscription_tier VARCHAR(50),
    is_trial BOOL,
    is_latest INT,
    previous_subscription_is_trial BOOL,
    subscription_start_date DATE,
    subscription_termination_date DATE,
    sku VARCHAR(100),
    sku_expiration_date DATE,
    is_family_plan BOOL,
    is_family_plan_parent BOOL,
    family_plan_parent_id BIGINT,
    country_code VARCHAR(5),
    subscription_type VARCHAR(30),
    subscription_days_lasted INT,
    gender VARCHAR(25),
    zipcode VARCHAR(30),
    birth_year INT,
    dt varchar(10)
    )
   diststyle even;