CREATE TABLE IF NOT EXISTS dw_royalties.custom_snapshot(
    profile_id BIGINT,
    reported_date VARCHAR(25),
    track_id INT,
    station_id VARCHAR(250),
    device_id VARCHAR(250),
    elapsed_time BIGINT,
    device_name VARCHAR(80),
    complete_stream_reason INT,
    subscription_tier VARCHAR(20),
    subscription_type VARCHAR(30),
    is_trial BOOL,
    sku VARCHAR(100),
    date_start VARCHAR(30),
    date_end VARCHAR(30),
    subscription_start_date VARCHAR(20),
    country_code VARCHAR(5),
    record_provider_id INT,
    record_provider_name VARCHAR(100),
    isrc VARCHAR(50),
    upc VARCHAR(30),
    grid VARCHAR(30),
    host VARCHAR(50),
    bitrate INT,
    title VARCHAR(450),
    artist_name VARCHAR(450),
    deal_type VARCHAR(10),
    artist_title VARCHAR(500),
    trackBundleTitle VARCHAR(500),
    zipcode VARCHAR(30),
    gender VARCHAR(25),
    birth_year INT,
    is_offline BOOL,
    is_shuffle BOOL,
    trackBundleId INT,
    trackNumber BIGINT,
    volumeNumber INT ,
    recordLabelName VARCHAR(330),
    recordSublabelName VARCHAR(350),
    content_type VARCHAR(10),
    station_type VARCHAR(20),
    played_from INT,
    is_source_favorite_track_stationcreate BOOL,
    is_source_favorite_artist_stationcreate BOOL,
    favorites_profile_id INT,
    is_my_favorites INT,
    duration FLOAT8,
    registered_date VARCHAR(30),
    track_version VARCHAR(200),
    dt varchar(10)
    )
    diststyle even;