CREATE TABLE IF NOT EXISTS dw_royalties.live_snapshot (
  territory VARCHAR(5),
  call_letter VARCHAR(15),
  royalty_alias_call_letter VARCHAR(15),
  station_format VARCHAR(25),
  record_provider_id INT,
  stream_id INT,
  stream_esid VARCHAR(10),
  spin_start VARCHAR(20),
  spin_end VARCHAR(20),
  track_id INT,
  isrc VARCHAR(40),
  title VARCHAR(300),
  spins_match_title VARCHAR(80),
  profile_id BIGINT,
  subscription_tier VARCHAR(15),
  subscription_type VARCHAR(25),
  is_mobile INT,
  product_id INT,
  listener_id VARCHAR(550),
  user_id VARCHAR(450),
  cart_number INT,
  listen_duration INT,
  record_provider_name VARCHAR(60),
  deal_type VARCHAR(10),
  is_active BOOLEAN,
  artist_name VARCHAR(330),
  artist_id INT,
  recordLabelName VARCHAR(230),
  recordSublabelName VARCHAR(250),
  upc VARCHAR(25),
  trackBundleId INT,
  trackbundletitle VARCHAR(450),
  trackNumber BIGINT,
  volumeNumber INT, 
  is_offline BOOL,
  gender VARCHAR(10),
  zipcode VARCHAR(10),
  birth_year INT,
  duration FLOAT8,
  registered_date VARCHAR(30),
  track_version VARCHAR(300),
  dt varchar(10)
)
diststyle even;