CREATE TABLE IF NOT EXISTS dw_royalties.fct_live_radio_track_users(
    session_duration INT,
    spin_duration INT,
    listen_duration INT,
    session_start TIMESTAMP,
    session_end TIMESTAMP,
    elapsed_time INT,
    is_mobile INT,
    cp_code INT,
    stream_id INT,
    call_letters VARCHAR(100),
    c_ip VARCHAR(500),
    profile_id BIGINT,
    uid VARCHAR(500),
    status_code INT,
    user_id VARCHAR(500),
    user_id_cume VARCHAR(500),
    user_agent VARCHAR(4000),
    init_id VARCHAR(200),
    listener_id VARCHAR(500),
    carrier VARCHAR(3000),
    client_type VARCHAR(500),
    device_id VARCHAR(500),
    device_pk INT,
    device_name VARCHAR(255),
    terminal_id INT,
    dist VARCHAR(500),
    territory VARCHAR(1000),
    lon VARCHAR(100),
    lat VARCHAR(100),
    fb_broadcast INT,
    iheartradioversion VARCHAR(500),
    os_version VARCHAR(500),
    played_from INT,
    camp_id VARCHAR(500),
    p_name VARCHAR(500),
    c_id VARCHAR(2000),
    at VARCHAR(100),
    item_count INT,
    mod_time VARCHAR(500),
    is_anonymous VARCHAR(10),
    is_triton INT,
    is_blacklist INT,
    amsparams_playerid VARCHAR(50),
    amsparams_skey INT,
    awparams_n INT,
    awparams_g INT,
    awparams_companionads VARCHAR(50),
    awparams_ccaud VARCHAR(4000),
    file_date VARCHAR(50),
    elapsed_time_raw BIGINT,
    defrag_count INT,
    source VARCHAR(20),
    spin_start TIMESTAMP,
    spin_end TIMESTAMP,
    call_letter VARCHAR(50),
    artist VARCHAR(250),
    title VARCHAR(250),
    duration FLOAT,
    mbid INT,
    cart_number INT,
    product_id INT,
    artist_id INT,
    event_type VARCHAR(50),
    esid INT,
    data_source VARCHAR(50),
    is_flashback BOOL,
    session_count INT,
    royalty_key VARCHAR(500),
    dt varchar(10)
)
 diststyle even;
