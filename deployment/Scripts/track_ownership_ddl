CREATE TABLE IF NOT EXISTS dw_royalties.track_ownership (
    record_provider_id INT,
    record_provider_name VARCHAR(80),
    deal_type VARCHAR(10),
    is_active BOOLEAN,
    id INT,
    artist_name VARCHAR(300),
    artist_title VARCHAR(420),
    title VARCHAR(300),
    isrc VARCHAR(20),
    upc VARCHAR(20),
    grid VARCHAR(25),
    trackBundleId INT,
    trackBundleTitle VARCHAR(450),
    trackNumber BIGINT,
    volumeNumber INT ,
    recordLabelName VARCHAR(280),
    recordSublabelName VARCHAR(280),
    duration FLOAT8,
    registered_date VARCHAR(30),
    track_version VARCHAR(220),
    dt varchar(10)
    )
 diststyle even;
 
